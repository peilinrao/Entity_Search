					var alinks_link;
					var trackImage = new Image();
					function alinks_click(link) {
						var keyphrase = link.innerHTML;
						trackImage.src = 'http://www.askageek.com/wp-content/plugins/alinks/classes/aLinksAjax.php?log=' + keyphrase;
						link.target = '_blank';
						pause(1000);
						return true;
					}
						
					function pause(numberMillis) {
					    var now = new Date();
					    var exitTime = now.getTime() + numberMillis;
					    while (true) {
							now = new Date();
							if (now.getTime() > exitTime) return;
						}
					}
