				} else {
					output += "<a style=\"color: black; text-decoration: none;\">"+radlinks[i].term+"</a>";
				}
				output += "</li>\n";
			}
			document.write("<ul class=\"quicklist\" style=\"overflow:visible; white-space: nowrap;\">" + output + "</ul>\n");
		}
		
