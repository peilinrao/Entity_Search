			if (radlinks.length < 1)
				return;

			var output = "";
			var rad_terms = "";
			var rad_rts = "";

			for(i=0; i < radlinks.length; ++i) {
				if (rad_terms.length > 0) {
					rad_terms += "|" + radlinks[i].term;
				} else {
					rad_terms += radlinks[i].term;
				}

				if (rad_rts.length > 0) {
					rad_rts += "|" + radlinks[i].radlink_token;
				} else {
					rad_rts += radlinks[i].radlink_token;
				}
			}

			for(i=0; i < radlinks.length; ++i) {
				output += "<li>";
				if (radlinks[i].term != 'null') {
					output += "<a target=\"_top\" href=\"http://www.apartmentratings.com/rate/radlinks-ads";
