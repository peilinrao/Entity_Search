	$(document).ready(function() {	
			
	/*
	 * added by deepak
	 *	check if upcoming checkbox is checked then disable publish date
	 */
	 
	$("input[@name='upcoming']").click(function(){
		if($('#upcoming').is(':checked')){
			 $("#fromdate").val('');
			 $("#todate").val('');
			 $("#upcoming_div").show();
			 $("#publish_div").hide();
			 
			 
		}else{
			  $("#upcoming_div").hide();
			  $("#publish_div").show();
			   $("#upcoming_fromdate").val('');
			 $("#upcoming_todate").val('');
			 
		}
	});
	
});


</script>


