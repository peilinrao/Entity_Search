	<script type="text/javascript">
	jQuery(document).ready(function() {
		jQuery('#find-it form select').dropkick();
		
				 degree = "";
		 campus = "";
		 delivery = "";
		 area = "";
		 academic_level = "";
		 
		
		
		if (academic_level != "blank")
		{
			acad_level = 'input:radio[value="' + academic_level + '"]';
			
			jQuery('form.searching-for-degrees input:radio[name="academic-level"]').filter(acad_level).prop('checked', true);
			
			if (academic_level == "graduate")
			{
				jQuery("#degrees, .degrees").show();				
			}
		}
		
		if (degree != "blank")
		{
			specific_degree = 'option[value="' + degree + '"]';
			jQuery('form.searching-for-degrees option').filter(specific_degree).attr("selected", "selected");
		}
		if (campus != "blank")
		{
			specific_campus = 'option[value="' + campus + '"]';
			jQuery('form.searching-for-degrees option').filter(specific_campus).attr("selected", "selected");
		}
		if (delivery != "blank")
		{
			specific_delivery = 'option[value="' + delivery + '"]';
			jQuery('form.searching-for-degrees option').filter(specific_delivery).attr("selected", "selected");
		}
		if (area != "blank")
		{
			specific_area = 'option[value="' + area + '"]';
			jQuery('form.searching-for-degrees option').filter(specific_area).attr("selected", "selected");
		}
		
		jQuery('.searching-for-degrees select').delay(1000).dropkick();
		jQuery('.indiv').submit(function(event) {
			event.preventDefault();
		 jQuery('.results').empty().append('<div style="width:400px;margin:150px auto;"><img src="/wp-content/themes/antioch - new/css/images/ajax-loader.gif" /></div>');
			
			dataString = jQuery('.indiv').serialize();
			jQuery.ajax({
			type: "POST",
			url: "/directory/",
			data: dataString,
		/*	beforeSend: function() {
			   alert("roar");
			  }, */ 
			success: function(data) {
					var content2 = jQuery(data).find('.results');
					jQuery('.results').replaceWith(content2);
				//	alert('it worked');
					},
		  error: function(){
		      alert('error happens');}
			});
			
		});
		jQuery('.searching-for-degrees').submit(function(event) {
			event.preventDefault();
			dataString = jQuery('.searching-for-degrees').serialize();
			jQuery.ajax({
			type: "POST",
			url: "/academics/find-your-program/search/",
			data: dataString,
		/*	beforeSend: function() {
			   alert("roar");
			  }, */ 
			success: function(data) {
					var content = jQuery(data).find('#article');
					jQuery('#article').empty().replaceWith(content);
				//	alert('it worked');
					},
      error: function(){
          alert('error happens');}
			});
			
		}); 
/*		jQuery('.searching-for-degrees input, .searching-for-degrees select').change(function() {
			
			dataString = jQuery('.searching-for-degrees').serialize();
			jQuery.ajax({
			type: "POST",
			url: "/academics/find-your-program/search/",
			data: dataString,  
			success: function(data) {
					var content = jQuery(data).find('#article');
					jQuery('#article').empty().append(content);
				//	alert('it worked');
					},
		  error: function(){
		      alert('error happens');}
			});
			
		}); */
		jQuery.localScroll();
	});
	
	    
	
	</script>
