<!-- output captured js for page -->
<script type="text/javascript">
(function($){
$('.quickViewBtn').attr('im_active', false);
$('.quickViewBtn').click(function(){
	var $obj = $(this);
	var $all = $('.quickViewBtn');
	var $ifo = $obj.parent().siblings(":next").children(":last");
	var vis  = $obj.hasClass('quickViewBtnActive');
	$all.each(function(){
		$(this).removeClass('quickViewBtnActive')
			.parent()
			.siblings(":next")
			.children(":last")
			.slideUp('normal');
	});
	if(vis){
		$obj.removeClass('quickViewBtnActive');
		$ifo.slideUp('normal').html('');
	}else{
		$obj.addClass('quickViewBtnActive');
		$.ajax({
			type: 'POST',
			url:  '/ad_summary.php',
			data: 'id=' + $obj.attr('item_id'),
			success: function(response){
				$ifo.html(response).slideDown('normal');
			}
		});
	}
});
})(jQuery);
</script>

