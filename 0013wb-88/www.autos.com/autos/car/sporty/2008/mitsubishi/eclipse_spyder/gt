lDropName];selModel = modelDrop.selectedIndex;changedMake( formName, makeDropName, modelDropName, yearDropName, selModel, initialModelId );} else {modelDrop = document.forms[formName].elements[modelDropName];modelDrop.selectedIndex=0;}if ( selModel > 0 ) {// ???} else {yearDrop = document.forms[formName].elements[yearDropName];if (yearDrop) {yearDrop.selectedIndex=0;}}}function changedMake( formName, makeDropName, modelDropName, yearDropName, selModel, initialModelId ) {makeDrop = document.forms[formName].elements[makeDropName];modelDrop = document.forms[formName].elements[modelDropName];selMake = makeDrop.selectedIndex;modelDrop.length=0;var modelPos=0;modelDrop.options[modelPos++]=new Option('Select A Model','');var allValidYears="";if((formName=='getaprice')&&(selMake>0)){for(i=1;i<=selMake;i++){var modelsString = evd_a[(i-1)*3+2];if(modelsString.indexOf("~")<0){selMake++;}}}if(selMake>0){evd_b=(evd_a[(selMake-1)*3+2]).split('/');if((initialModelId>0) && !(formName=='getaprice')) {modelDrop.options[modelPos++]=new Option('See All Models','');}for (i=0;i<evd_b.length/3;i++){var modelName = evd_b[i*3];var modelId = evd_b[i*3+1];var thisModelsValidYears = evd_b[i*3+2];allValidYears += thisModelsValidYears;if((formName!='getaprice')||(thisModelsValidYears.indexOf("~")>0)){modelDrop.options[modelPos++]=new Option(modelName,modelId);}if ( selModel != "" && modelId == selModel ) {selModel = i+initialModelId;}}if (yearDropName){yearDrop = document.forms[formName].elements[yearDropName];buildYear(yearDrop, allValidYears);}}modelDrop.selectedIndex=selModel;}function changedModel( formName, makeDropName, modelDropName, yearDropName, selModel, initialModelId ) {makeDrop = document.forms[formName].elements[makeDropName];modelDrop = document.forms[formName].elements[modelDropName];yearDrop = document.forms[formName].elements[yearDropName];selMake = makeDrop.selectedIndex;selModel = modelDrop.selectedIndex;yearDrop.length=0;yearDrop.options[0]=new Option('Year','');var validYears="";if(selMake>0){evd_b=(evd_a[(selMake-1)*3+2]).split('/');if(selModel>1){validYears = evd_b[ ((selModel-2)*3)+2 ];} else {for (i=0;i<evd_b.length/3;i++){validYears+=evd_b[i*3+2];}}buildYear( yearDrop, validYears );}}function buildYear(yearDrop, validYears){yearDrop.length=0;yearDrop.options[0]=new Option('Year','');yearPos=1;for(i=yearChars.length-1;i>=0;i--){if(validYears.indexOf(yearChars.charAt(i))>=0){yearDrop.options[yearPos]=new Option((""+(i+yearStart)),(i+yearStart));yearPos++;}}}function checkMake(whichform) {return getSelectedMake(whichform) != null;}function alertMake(whichform) {if( ! checkMake(whichform) ) {alert( 'Please select a make first.' );return false;}return true;}function checkModel(whichform) {return getSelectedModel(whichform) != null;}function getSelectedModel(whichform) {var result = document.forms[whichform].modelid.options[document.forms[whichform].modelid.selectedIndex].value;return (result == "" || result == '-1') ? null : result;}function getSelectedMake(whichform) {var result = document.forms[whichform].make.options[document.forms[whichform].make.selectedIndex].value;return result == "" ? null : result;}function Go2(whichform) {if (!checkMake(whichform)) {alert('Please select Make.');return;}if (document.forms[whichform].modelid.selectedIndex <= 1) {document.forms[whichform].action = "/autos/models";}document.forms[whichform].target = "_top";document.forms[whichform].submit();}initMake('autos','make','modelid', 'year', '', '');//--></script><IFRAME id="AutosPrompt" style="position:absolute;width:0;height:0;visibility:hidden;z-index:5000;" FRAMEBORDER="2" BORDERCOLOR="#ffffff" src="https://cdcssl.ibsrv.net/common/blank.htm"></IFRAME><!-- ibpxl --><script type="text/javascript"> pxlSiteFile = "autos.com.js"; if (location.hostname.indexOf("welcome") > -1) pxlSiteFile = "welcome.autos.com.js"; pxlScriptStart = '<scr