      function checkSearchForm(){
        var tmpSearch = $('lilQ').value.trim();
        if (tmpSearch == 'Search Site')
          $('lilQ').value = '';
      }
      function textPrompter(el, focused){
        if (focused && el.value == 'Search Site'){
          el.value = '';
          el.removeClassName('dimSearch');
        }else{
          if (!focused && el.value == ''){
            el.value = 'Search Site';
            el.addClassName('dimSearch');
          }
        }
      }
