                                    items: new Ext.Panel({
                                        border: false,
                                        autoScroll: true,
                                        padding: 10,
                                        html: panel.body.dom.innerHTML
                                      }),
                                    buttons: [{text: 'Close', handler: function(p, e){win.close();}}]
                                  });
                                win.show();
                                
                              }
                          };
      
