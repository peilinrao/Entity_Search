  <script type="text/javascript">
    
    var searchBar;
    function submitSearch(){
      document.forms["searchForm"].submit();
    }
    function showSearchBar(){
      Ext.get('searchHolder').removeClass('x-hidden');
      Ext.get('toolsHolder').addClass('x-hidden');
      resizePagerBar();
      if (searchBar)
        searchBar.focus();
    }
    function hideSearchBar(){
      Ext.get('searchHolder').addClass('x-hidden');
      Ext.get('toolsHolder').removeClass('x-hidden');
      resizePagerBar();
    }
    function resizePagerBar(){
      var srchBtn = Ext.get('searchButtonMain');
      var srchCnclBtn = Ext.get('searchButtonCancel');
      var logo = Ext.get('pagerBarToolLogo');
      var item = Ext.get('pagerBar');
      //Ext.get('lilQ').setWidth(item.getWidth() - logo.getWidth() - srchBtn.getWidth() - srchCnclBtn.getWidth() - 20);
      if (searchBar)
        searchBar.setWidth(item.getWidth() - logo.getWidth() - srchBtn.getWidth() - srchCnclBtn.getWidth() - 20);
    }
    Ext.onReady(function(){
      window.onresize = resizePagerBar;
      /*
      var el = Ext.get('lilQ');
      el.on('keydown', function(e){
        window.location.href = "#!/"+ currentSchool.Alias + "/Search/" + this.dom.value;
      }, el, {buffer: 300});
      */
        
      var ds = new Ext.data.Store({
          url: '/ajax/search/searchSuggestion.cfm',
          reader: new Ext.data.JsonReader({
              root: 'results'
          }, [
              {name: 'title', mapping: 'title'}
          ])
      });

      // Custom rendering Template
      var resultTpl = new Ext.XTemplate(
          '<tpl for="."><div class="search-item">',
              '{title}',
          '</div></tpl>'
      );

      searchBar = new Ext.form.ComboBox({
        applyTo: 'lilQ',
        autoShow: false,
        store: ds,
        mode: 'remote',
        displayField:'title',
        typeAhead: false,
        loadingText: 'Getting Suggestions...',
        hideTrigger:true,
        tpl: resultTpl,
        itemSelector: 'div.search-item',
        onSelect: function(record){
            window.location.href = "#!/"+ currentSchool.Alias + "/Search/" + record.get('title');
        },
        listeners: {
          show: function(){
            resizePagerBar();
          }
        }
      });
      searchBar.show();
      var el = Ext.get('lilQ');
      el.on('keydown', function(e){
        window.location.href = "#!/"+ currentSchool.Alias + "/Search/" + this.dom.value;
      }, el, {buffer: 300});
    });
  </script>

