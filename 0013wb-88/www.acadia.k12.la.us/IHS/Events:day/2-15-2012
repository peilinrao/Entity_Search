                  c.setTitle('Widget Error');
                  c.body.dom.innerHTML = '';
                  c.body.dom.appendChild(errorDiv);
                },
                load: function(s,r,o){
                  if (c){
                    if (hideLoad && r.length > 0){
                      c.setTitle(r[0].data.title);
                      c.body.dom.innerHTML = r[0].data.data;
                    }else{
                      if (c.body){
                        c.setTitle("Loading...");
                        c.body.dom.innerHTML = c.loadingText;
                      }
                      if (r.length > 0){
                        setTimeout(function(){
                          c.setTitle(r[0].data.title);
                          c.body.dom.innerHTML = r[0].data.data;
                        }, 500);
                      }
                    }
                  }
                }
              }            
            });
      });
    */
      
    var printBox;
    var doPrint;
    Ext.onReady(function(){
      doPrint = function(link){
        printBox = new Ext.Window({
          title: 'Print',
          modal: true,
          resizable: false,
          buttonAlign: 'center',
          layout: 'border',
          height: 170,
          width: 400,
          items: [
                    {
                      bodyStyle: 'padding: 10px; text-align: center; font-weight: bold',
                      region: 'north',
                      border: false,
                      height: 30,
                      html: 'What portion of the page would you like to print?'
                    },{
                      region: 'center',                    
                      border: false,
                      layout: {
                                type:'vbox',
                                padding:'5',
                                align:'stretch'
                              },
                      items:[{
                                  xtype:'button',
                                  cls: 'bigPrint1',
                                  text: 'Print Entire Page',
                                  handler: function(){
                                    printBox.close();
                                    Ext.Msg.hide();
                                    window.print();
                                  }
                              },{
                                  xtype:'spacer',
                                  flex: 1
                              },{
                                  xtype:'button',
                                  cls: 'bigPrint2',
                                  text: 'Print Content Only',
                                  handler: function(){
                                    printBox.close();
                                    Ext.Msg.hide();
                                    Ext.Msg.wait('Getting ready to print..','Please Wait...');
                                    //newLocation = link.href.replace('#','') + '&printview=1';
                                    newLocation = '/print'+lastLoadedURLRel;
                                    //console.info(newLocation);
                                    setTimeout(function(){
                                      var newWindow = window.open(newLocation, '_blank');
                                      newWindow.focus();
                                      Ext.Msg.hide();
                                    }, 300);
                                    
                                  }
                              }]
                    }
                ],

          buttons: [
                  {
                    text: 'Cancel',
                    handler: function(){ 
                      printBox.close();
                    }
                  }
                ]
        
        });
        printBox.show(this);
      }
    });
    

    currentPage.storeParams = {
      dataID: currentPageDataID, 
      alias: currentPageAction, 
      keyword: currentPageKeyword, 
      curPage: currentPageNumber, 
      id: currentPageModuleID, 
      portletID: 0, 
      portalID: 0
    }; 
    currentPage.values = {dateoffset: '', layout: '', maxcalendarlistdays: '', maxcalendarlistitems: '', __alias: 'undefined'};
  </script>
  
  
  
  <div class="pagerBarHolder pagerBarTop">
    <script type="text/javascript">
  var siteNotice = false;
  function dropSiteNotice(){
    new Ajax.Request('/supressMessages.cfm?supress=false');
    Effect.BlindDown('siteNoticeBoxHolder', { duration: 0.5 })
    $('pagerBarToolAlertButton').addClassName('buttonOn');
  }
  function liftSiteNotice(){
    new Ajax.Request('/supressMessages.cfm?supress=true');
    Effect.BlindUp('siteNoticeBoxHolder', { duration: 0.25 })
    $('pagerBarToolAlertButton').removeClassName('buttonOn');
  }
  function toggleSiteNotice(){ 
    
    if ($('pagerBarToolAlertButton').hasClassName('buttonOn')){
      liftSiteNotice();
    }else{
      dropSiteNotice();
    }
  
  }
</script>

