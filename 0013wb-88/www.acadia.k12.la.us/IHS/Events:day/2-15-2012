    <script type="text/javascript">
      var moduleList = {
        userLogin: { name: 'User Login', wait: false, func: 'userLogin', reported: false, loaded: false },
        userLoginIES: { name: 'IES Special Login', wait: false, func: 'userLoginAs', reported: false, loaded: false },
        userLoginAs: { name: 'Login As...', wait: false, func: 'userLoginAs', reported: false, loaded: false },
        userSwitch: { name: 'Switch Users', wait: false, func: 'userSwitch', reported: false, loaded: false },
        userLogout: { name: 'User Logout', wait: false, func: 'userLogout', reported: false, loaded: false },
        manageGalleries: { name: 'Gallery Manager', wait: false, func: 'manageGalleries', reported: false, loaded: false, needsLoader: true },
        userGroups: { name: 'Groups & Users', wait: false, func: 'windowUserGroups', reported: false, loaded: false, needsLoader: true },
        userGCDMembers: { name: 'Members', wait: false, func: 'windowUserGroups', reported: false, loaded: false, needsLoader: true },
        siteConfig: { name: 'Site Config', wait: false, func: 'windowSiteConfig', reported: false, loaded: false, needsLoader: true, requires: ['generalTab', 'navTab', 'schoolTab', 'gcdTab', 'gcdUserAssignDlg', 'forumTab', 'catTab'] }
      };
      
      function checkModuleLoaded(moduleName){
        if (!moduleList[moduleName])
          return false;
        if (moduleList[moduleName].requires && moduleList[moduleName].requires.length > 0){
          var mods = moduleList[moduleName].requires;
          var modLen = mods.length;
          loaded = moduleList[moduleName].reported;
          var nn=0;
          for(var n=0; n<modLen; n++){
            if (!moduleList[mods[n]] || !moduleList[mods[n]].loaded){
              loaded = false;
            }else{
              nn++;
            }
          }
          moduleList[moduleName].loadedCt = nn;
          moduleList[moduleName].loaded = loaded;
        }else{
          if (moduleList[moduleName].reported)
            moduleList[moduleName].loaded = true;
        }
      }
      function launchModule(moduleName, aargs){
        if (!moduleList[moduleName] || !moduleList[moduleName].func)
          return false;
                
        //alert(moduleName);
        //alert(moduleList[moduleName].toSource());
                
        if (moduleList[moduleName].loaded){
          if (moduleList[moduleName].wait && !moduleList[moduleName].needsLoader){
            Ext.onReady(function(){ 
              Ext.Msg.updateText('Launching '+moduleList[moduleName].name+'...');
            });
          }
          if (moduleList[moduleName].needsLoader){
            Ext.onReady(function(){ 
              if (moduleList[moduleName].wait)
                Ext.Msg.hide();
              moduleList[moduleName].wait=false;
              Ext.Msg.wait('Launching '+moduleList[moduleName].name+'...', moduleList[moduleName].name);
            });
          }
          clearTimeout(moduleList[moduleName].timer);
            
          setTimeout(function(){ 
            var myModule = moduleList[moduleName];
            Ext.onReady(function(){ 
              if (myModule.wait)
                Ext.Msg.hide();
            });
            myModule.wait=false;
            if (myModule.func && window[myModule.func])
              window[myModule.func].createDelegate(this, aargs)();
              //window[myModule.func].apply(this, aargs);
          }, 500);
        }else{
          clearTimeout(moduleList[moduleName].timer);
          if (maxLoadTimePassed){
            Ext.onReady(function(){ 
              Ext.Msg.hide();
            });
            moduleList[moduleName].wait=false;
            if (moduleList[moduleName].requires && moduleList[moduleName].requires.length > 0){
              var curLoad = moduleList[moduleName].loadedCt || 0;
              var modLen = moduleList[moduleName].requires.length;
              Ext.onReady(function(){ 
                Ext.Msg.alert('Error Loading '+moduleList[moduleName].name+' ['+curLoad+'/'+modLen+']', moduleList[moduleName].name+' failed to load due to a connection problem. Please try again in a few seconds.');
              });
            }else{
              Ext.onReady(function(){ 
                Ext.Msg.alert('Error Loading '+moduleList[moduleName].name, moduleList[moduleName].name+' failed to load due to a connection problem. Please try again in a few seconds.');
              });
            }
            return false;
          }
          if (!moduleList[moduleName].wait){
            Ext.onReady(function(){ 
              Ext.Msg.wait('Please wait...', 'Loading '+moduleList[moduleName].name);
            });
            moduleList[moduleName].wait=true;
          }
          if (moduleList[moduleName].requires && moduleList[moduleName].requires.length > 0){
            var curLoad = moduleList[moduleName].loadedCt || 0;
            var modLen = moduleList[moduleName].requires.length;
            Ext.onReady(function(){ 
              Ext.Msg.updateText('Loaded ['+curLoad+' of '+modLen+'] helpers. Please wait...');
            });
          }
          moduleList[moduleName].timer = setTimeout(function(){ launchModule(moduleName, aargs); }, 100);
        }
      }
      var normalLoadTimePassed = false;
      var maxLoadTimePassed = false;
      setTimeout(function(){ normalLoadTimePassed=true }, 10000);
      setTimeout(function(){ maxLoadTimePassed=true }, 30000);
      function reportingIn(moduleName){

        if(!moduleList[moduleName])
          moduleList[moduleName] = {slave: true};
          
        var thisModule = moduleList[moduleName];
        
        thisModule.reported = true;
        
          var found = false;
          var foundReq = "";
          for(var mod in moduleList){
            checkModuleLoaded(mod);
            var eMod = moduleList[mod];
            if (eMod.requires && eMod.requires.length > 0){
              var mods = eMod.requires;
              var modLen = mods.length;
              for(var n=0; n<modLen; n++){
                if (mods[n] == moduleName){
                  foundReq = mods[n];
                  found = true;
                  break;
                }
              }
            }
          }
        
        Ext.onReady(function(){
          if (found && foundReq && moduleList[foundReq] && moduleList[foundReq].loaded){
            var nodes = Ext.query('.loadingIcon_'+foundReq);
            for (var c=0;c<nodes.length;c++)
              Ext.fly(nodes[c]).removeClass('loadingIcon');
          }
          for(var mod in moduleList){
            if (moduleList[mod].loaded == true){
              var nodes = Ext.query('.loadingIcon_'+mod);
              for (var c=0;c<nodes.length;c++)
                Ext.fly(nodes[c]).removeClass('loadingIcon');
            }
          }
          if (thisModule.loaded){
            var nodes = Ext.query('.loadingIcon_'+moduleName);
            for (var c=0;c<nodes.length;c++)
              Ext.fly(nodes[c]).removeClass('loadingIcon');
          }
        });

      }
</script> 
