<script type="text/javascript">
var strSearchSort = "desc";
var urlSearchSave = '';
                         
var currentUrl = '/search/q-Dry-Van';
var strHiddenQueryString = 'searchKw=Dry-Van';
function setMaxResults(maxresults){
	document.location.href = currentUrl + '?' + urlSearchSave + 'page=1&limit=' + maxresults;
}	

function viewType(type){
            document.location.href = currentUrl + '?' + urlSearchSave + 'viewtype='+type;
}
