<script type="text/javascript">

function clearSearchFields(id){
    element = document.getElementById(id);
        
		if(element.value == 'Type keywords (Dry Van, Teams, etc)'){
                    element.value = '';
                }

                if(element.value == 'Type Location (City, State or Zip Code)'){
                    element.value = '';
                }
}

function checkSearchFields(id){
    element = document.getElementById(id);

		if(element.value == '' && id == 'searchkw'){
                    element.value = 'Type keywords (Dry Van, Teams, etc)';
                }

                if(element.value == '' && id == 'location'){
                    element.value = 'Type Location (City, State or Zip Code)';
                }
}

</script>

<div class="searchbox">

    <form method="post" action="" autocomplete="off" onsubmit="cleanBlanks()" id="searchform">
                <div class="kw">
                	<input id="searchkw" type="text" maxlength="200" name="searchKw" autocomplete="off" value="Type keywords (Dry Van, Teams, etc)" onclick="clearSearchFields('searchkw')" onblur="checkSearchFields('searchkw')"/><br/>
                    
                </div>

                <div class="location">
                	<input id="location" maxlength="200" type="text" name="location" value="Type Location (City, State or Zip Code)" onclick="clearSearchFields('location')" onblur="checkSearchFields('location')"/>
                                <input type="hidden" name="Country" id="hiddenCountry" value="US"/>
                </div>
                
                
