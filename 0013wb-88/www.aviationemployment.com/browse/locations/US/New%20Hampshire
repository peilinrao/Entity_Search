		<script type="text/javascript">

function clearSearchFields(id){
    element = document.getElementById(id);
        
		if(element.value == 'Type keywords (e.g. Pilot, Technician)'){
                    element.value = '';
                }

                if(element.value == 'Type Location (City, State or Zip Code)'){
                    element.value = '';
                }
}

function checkSearchFields(id){
    element = document.getElementById(id);

		if(element.value == '' && id == 'searchkw'){
                    element.value = 'Type keywords (e.g. Pilot, Technician)';
                }

                if(element.value == '' && id == 'location'){
                    element.value = 'Type Location (City, State or Zip Code)';
                }
}

</script>
