        function clearTextForSearch() {
                    if (document.getElementById('ctl00_Header1_txtKeySearch').value == "Keyword Search")
                    {
                        document.getElementById('ctl00_Header1_txtKeySearch').value = "";
                    }
                }

        function resetTextForSearch() {
                    if (document.getElementById('ctl00_Header1_txtKeySearch').value == "")
                    {
                        document.getElementById('ctl00_Header1_txtKeySearch').value = "Keyword Search";
                    }                
                }   
                
                
        function SearchonEnterHit()
        {
         	if (window.event.keyCode == 13)
         	{
         	document.getElementById('ctl00_Header1_imgKeySearch').click();
         	}
        }        
                
</script>
 
