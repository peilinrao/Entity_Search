    <script type="text/javascript" language='javascript'>  
     
        /* 
           this function finds the TOP position of element on page
        */
        
        function findTopPos(obj) {
	        var curleft = curtop = 0;
	        if (obj.offsetParent) {
		        do {
			        curleft += obj.offsetLeft;
			        curtop += obj.offsetTop;
		        } while (obj = obj.offsetParent);
	        }
	        return curtop;
        }

        /* 
           this function finds the LEFT position of element on page
        */
        function findLefPos(obj) {
	        var curleft = curtop = 0;
	        if (obj.offsetParent) {
		        do {
			        curleft += obj.offsetLeft;
			        curtop += obj.offsetTop;
		        } while (obj = obj.offsetParent);
	        }
	        return curleft;
        }
        
        

        function clearTextForSearch() {
            if (document.getElementById('ctl00_txtKeySearch').value == "Keyword Search")
            {
                document.getElementById('ctl00_txtKeySearch').value = "";
            }
        }

        function resetTextForSearch() {
            if (document.getElementById('ctl00_txtKeySearch').value == "")
            {
                document.getElementById('ctl00_txtKeySearch').value = "Keyword Search";
            }                
        }   
        
        function ShowLoginTabs(ntabIndex)
        {
            var dLogin = document.getElementById('dLogin');
            var dCreateAccount = document.getElementById('dCreateAccount');
                                       
            if (ntabIndex==1)
            {
                dLogin.style.display='block';
                dCreateAccount.style.display='none';
                        
                // className is used for IE and class for other than IE browsers
                document.getElementById("aLogin").setAttribute("className", "select");
                document.getElementById("aLogin").setAttribute("class", "select");

                document.getElementById("aCreateAccount").setAttribute("className", "");
                document.getElementById("aCreateAccount").setAttribute("class", "");
                
            }
            else if (ntabIndex==2)
            {
                dCreateAccount.style.display='block';
                dLogin.style.display='none';
                
                document.getElementById("aCreateAccount").setAttribute("className", "select");
                document.getElementById("aCreateAccount").setAttribute("Class", "select");
                
                document.getElementById("aLogin").setAttribute("className", "");
                document.getElementById("aLogin").setAttribute("class", "");
            }    
        }
           function Email()
        {        
            window.open('mailto:ryan27@aaronequipment.com');                
        }
        
        
    </script>

