    <script type="text/javascript" language="javascript">  
	
	function AddItemTeamOrders() {            
        
	       var Qty = document.getElementById("ctl00_cphActualSite_txtVolDisQty").value;
	       
	       var VolDisQty = document.getElementById("ctl00_cphActualSite_hfVolDisQty").value;
	      
	       if(Qty < VolDisQty)
	       {
	            alert("Please enter valid quantity for Team Orders");
	            return false;
	       }

	       Qty = Qty.toString();

	       for (i = 0; i < Qty.length; i++) {
	           var c = Qty.charAt(i);
	           if (isNaN(c)) {
	               alert("Please enter proper quantity value");
	               return false;
	           }
	       }

		  document.getElementById("ctl00_cphActualSite_imgBuyNowTeamOrders").click();
		   return true;
			 
	}
	
	   function AddItem() {

	       var Qty = document.getElementById("ctl00_cphActualSite_txtQty").value;

	       Qty = Qty.toString();

	       for (i = 0; i < Qty.length; i++) {
	           var c = Qty.charAt(i);
	           if (isNaN(c)) {
	               alert("Please enter proper quantity value");
	               return false;
	           }
	       }

		  document.getElementById("ctl00_cphActualSite_imgRequestQuoteLarge").click();
		   return true;			 
	}
		
	  function CheckRating(sender, args)
	{
		var text = document.getElementById('ctl00_cphActualSite_hdRating').value;
		var intVal = parseInt(text);     
		if(intVal == 0){
			 args.IsValid = false;    
			 return;        
		}
		document.getElementById('ctl00_cphActualSite_cvRating').style.visibility = "hidden";
		args.IsValid = true;    
	}

	function SetValue(val)
	{
		document.getElementById("ctl00_cphActualSite_hdRating").value = val;
		document.getElementById('ctl00_cphActualSite_cvRating').style.visibility = "hidden";
	}
			
	function DisplayDiv(ntabIndex)
	{
		ShowFirstTabDiv(ntabIndex);
		ShowSecondTabDiv(1);               
	}
	
	function DisplayDivForCart(ntabIndex)
	{
		ShowFirstTabDiv(ntabIndex);
		ShowSecondTabDiv(1);        
		
		try
		{
		   ShowPopUp();
		}
		catch(err)
		{
			//Handle errors here
		}
	}

	function FnOpenPrintProduct(ProductID) {
		 var url = '../../../PrintProduct.aspx?ID=' + ProductID;
		 var load = window.open(url, '', 'scrollbars=yes,menubar=no,height=310,width=620,resizable=no,toolbar=no,location=no,status=no');
		 }

	
	function DisplayFirstSecondDiv(nFirstDivtabIndex,nSecondDivtabIndex)
	{
		ShowFirstTabDiv(nFirstDivtabIndex);
		ShowSecondTabDiv(nSecondDivtabIndex);
	}
		
	function DisplayMessage(ntabIndex)
	{
		if (ntabIndex==2)
		{
			alert('Thank You for your Ask A Question enquiry. A representative will be in touch with you soon.');
		}        
		ShowSecondTabDiv(ntabIndex);
		ShowFirstTabDiv(1);
	}
	
	function ShowFirstTabDiv(ntabIndex)
		{
			var dDescription=document.getElementById('dDescription');
			var dProductDetails=document.getElementById('dProductDetails');
			var dSpecification=document.getElementById('dSpecification');
			
			var aDescription =document.getElementById("aDescription") ;
			var aProductDetails = document.getElementById("aProductDetails") ;
			var aSpecification = document.getElementById("aSpecification") ;
			
									   
			if (ntabIndex==1)
			{
				dDescription.style.display='block';                
				dProductDetails.style.display='none';
				dSpecification.style.display='none'; 
				
				if ( aDescription != null )
				{
				document.getElementById("aDescription").setAttribute("className", "active");
				document.getElementById("aDescription").setAttribute("class", "active");
				}

			  if ( aProductDetails != null ) {
				document.getElementById("aProductDetails").setAttribute("className", "");
				document.getElementById("aProductDetails").setAttribute("Class", "");
				}
				if ( aSpecification != null ) {
				document.getElementById("aSpecification").setAttribute("className", "");
				document.getElementById("aSpecification").setAttribute("Class", "");
				}                                                
				
			}
			else if (ntabIndex==2)
			{
				dProductDetails.style.display='block';
				
				dDescription.style.display='none';  
				dSpecification.style.display='none';    
				
				if ( aDescription != null ){
				document.getElementById("aDescription").setAttribute("className", "");
				document.getElementById("aDescription").setAttribute("class", "");
				 }
				 
				 if ( aProductDetails != null ) {
				document.getElementById("aProductDetails").setAttribute("className", "active");
				document.getElementById("aProductDetails").setAttribute("Class", "active");
				}
				if ( aSpecification != null ) {
				document.getElementById("aSpecification").setAttribute("className", "");
				document.getElementById("aSpecification").setAttribute("Class", "");
				}                                
				
			}
			else if (ntabIndex==3)
			{
				dSpecification.style.display='block';
				dDescription.style.display='none';                
				dProductDetails.style.display='none';    
				
				 if ( aDescription != null ){                 
				document.getElementById("aDescription").setAttribute("className", "");
				document.getElementById("aDescription").setAttribute("class", "");
				 }
				 
				   if ( aProductDetails != null ){
				document.getElementById("aProductDetails").setAttribute("className", "");
				document.getElementById("aProductDetails").setAttribute("Class", "");
				}
				
				  if ( aSpecification != null ){
				document.getElementById("aSpecification").setAttribute("className", "active");
				document.getElementById("aSpecification").setAttribute("Class", "active");
				}                                                
				
			}
			else if (ntabIndex==4)
			{
				
				dDescription.style.display='none';  
				dProductDetails.style.display='none'; 
				dSpecification.style.display='none';               
						 
			   if ( aDescription != null ){          
				document.getElementById("aDescription").setAttribute("className", "");
				document.getElementById("aDescription").setAttribute("Class", "");
			   }
			   
				if ( aProductDetails != null ){     
				document.getElementById("aProductDetails").setAttribute("className", "");
				document.getElementById("aProductDetails").setAttribute("Class", "");
				}
				if ( aSpecification != null ){     
				document.getElementById("aSpecification").setAttribute("className", "");
				document.getElementById("aSpecification").setAttribute("Class", "");
				}
				
			}
		}
				
		function ShowSecondTabDiv(ntabIndex)
		{
			var dVendor=document.getElementById('dVendor');
			var dAskAQuestion=document.getElementById('dAskAQuestion');
					
			
			if (ntabIndex==1)
			{
				dVendor.style.display='block';                
				
				dAskAQuestion.style.display='none';                
				
				

				
				document.getElementById("aVendor").setAttribute("className", "active");
				document.getElementById("aVendor").setAttribute("class", "active");

				document.getElementById("aAskaQuestion").setAttribute("className", "");
				document.getElementById("aAskaQuestion").setAttribute("Class", "");                
				
				
				
				
			}
			else if (ntabIndex==2)
			{
				dAskAQuestion.style.display='block';
				dVendor.style.display='none';   
				
				
				
				
				document.getElementById("aVendor").setAttribute("className", "");
				document.getElementById("aVendor").setAttribute("class", "");

				document.getElementById("aAskaQuestion").setAttribute("className", "active");
				document.getElementById("aAskaQuestion").setAttribute("Class", "active"); 
				
				
				
				
			}
			else if (ntabIndex==3)
			{
				dAskAQuestion.style.display='none';
				dVendor.style.display='block';   
				
				
				
				
				document.getElementById("aVendor").setAttribute("className", "active");
				document.getElementById("aVendor").setAttribute("class", "active");

				document.getElementById("aAskaQuestion").setAttribute("className", "");
				document.getElementById("aAskaQuestion").setAttribute("Class", ""); 
				
				
				
				
				
			}
			else if (ntabIndex==4)
			{                          
				dAskAQuestion.style.display='none';
				dVendor.style.display='block';   
				
				
				
				
				document.getElementById("aVendor").setAttribute("className", "active");
				document.getElementById("aVendor").setAttribute("class", "active");

				document.getElementById("aAskaQuestion").setAttribute("className", "");
				document.getElementById("aAskaQuestion").setAttribute("Class", ""); 
				
				
				
				
				
			}
		  
		}
		
		function CheckQuestionComments(sender, args)
		{    
			if(event.type == 'click')
			{
				var text = document.getElementById('ctl00_cphActualSite_txtQuestionComments').value;
				text = Trim(text);
				if (text.length == 0){        
					 args.IsValid = false;    
					 return;
				}
				if (!checkLength(text)){
					 alert("The length of comments entered is more than 1000 characters.");
					 args.IsValid = false;    
					 return;
				}
				args.IsValid = true;
			}     
		}
	
	function Trim(str){
		while (str.substring(0,1) == ' '){ // check for white spaces from beginning
			str = str.substring(1, str.length);
		}
		while (str.substring(str.length-1, str.length) == ' '){  // check white space from end
			str = str.substring(0,str.length-1);
		}
		return str;
	}

	function checkLength(text){
		max = 1000;
		 if (text.length > max) {
		  return false;
		 }
		return true;
	}
	
